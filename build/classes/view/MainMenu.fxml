<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="controller.MainMenuController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="400.0">
         <children>
            <Label fx:id="labelMainMenu" alignment="CENTER" contentDisplay="CENTER" prefHeight="66.0" prefWidth="600.0" text="Main Menu">
               <font>
                  <Font name="System Bold" size="26.0" />
               </font>
            </Label>
            <Button fx:id="buttonCustomers" contentDisplay="CENTER" maxWidth="-Infinity" mnemonicParsing="false" onAction="#onActionCustomersButton" prefWidth="95.0" text="Customers">
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="buttonAppointments" maxWidth="-Infinity" mnemonicParsing="false" onAction="#onActionAppointmentsButton" prefWidth="95.0" text="Appointments">
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="buttonReports" maxWidth="-Infinity" mnemonicParsing="false" onAction="#onActionReportsButton" prefWidth="95.0" text="Reports">
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="buttonExit" maxWidth="-Infinity" mnemonicParsing="false" onAction="#onActionExitButton" prefWidth="95.0" text="Exit">
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
